{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/api/comments", "dest": "/api/comments.js" },
    { "src": "/api/like", "dest": "/api/like.js" },
    { "src": "/api/user", "dest": "/api/user.js" },
    { "src": "/api/(.*)", "dest": "/api/index.js" }
  ]
}
